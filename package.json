{
  "name": "node_boilerplate",
  "version": "1.0.0",
  "description": "Node js Boilerplate",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "build": "babel ./src/ --out-dir dist/ --ignore ./node_modules,./src/ui,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "serve": "json-server db.json",
    "start": "npm run build && nodemon dist/index.js",
    "build-ui": "webpack --watch",
    "run-all": "run-p serve build-ui start"
  },
  "dependencies": {
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "express": "^4.15.4",
    "gmail-send": "^1.2.11",
    "google-auth-library": "^0.11.0",
    "googleapis": "^21.3.0",
    "jest": "^20.0.4",
    "morgan": "^1.8.2",
    "node-cron": "^1.2.1",
    "nodemon": "^1.12.0",
    "npm-run-all": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "draft-js": "^0.10.1",
    "draftjs-to-html": "^0.7.5",
    "fin-hypergrid": "^2.0.2",
    "golden-layout": "^1.5.9",
    "immutability-helper": "^2.3.1",
    "isomorphic-fetch": "^2.2.1",
    "json-server": "^0.12.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-draft-wysiwyg": "^1.10.9",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5"
  }
}
